<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Name="but1" Content="Button" HorizontalAlignment="Left" Margin="160,79,0,0" VerticalAlignment="Top" Click="Button_Click"/>
        <Ellipse Name="Ellipse1" HorizontalAlignment="Left" Height="95" Margin="180,123,0,0" Stroke="Black" VerticalAlignment="Top" Width="162" RenderTransformOrigin="0.5,0.5">
            <Ellipse.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleY="-15.944"/>
                    <RotateTransform/>
                    <TranslateTransform Y="-4.285"/>
                </TransformGroup>
            </Ellipse.RenderTransform>
        </Ellipse>
        <TextBlock Name="text1" HorizontalAlignment="Left" Margin="492,89,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>

    </Grid>
</Window>
